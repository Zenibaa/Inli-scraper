{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "mvn clean package",
    "runCommand": "java -cp target/classes com.inliscraper.App"
  },
  "deploy": {
    "startCommand": "java -cp target/classes com.inliscraper.App",
    "restartPolicyType": "ON_FAILURE"
  }
}
